<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="120"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="0">
            <shape
                android:shape="line">

                <stroke
                    android:width="1dp"
                    android:color="@color/colorPrimary"/>
            </shape>
        </rotate>
    </item>
</layer-list>